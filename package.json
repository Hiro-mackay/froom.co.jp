{
  "name": "froom.co.jp",
  "description": "Web site of froom.co.jp by Gatsby.js",
  "version": "1.0.0",
  "author": "Jphn Mackay",
  "scripts": {
    "lint": "next lint",
    "predev": "npm run tailwind:dev",
    "dev": "next dev",
    "start": "next start",
    "prebuild": "npm run tailwind:build",
    "build": "next build && node .next/server/build-rss.js && node .next/server/generateSitemap.js",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "tailwind:dev": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.compiled.css",
    "tailwind:build": "NODE_ENV=production npx tailwindcss-cli@latest build src/styles/tailwind.css -o src/styles/tailwind.compiled.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/globby": "^9.1.0",
    "@zeit/react-jsx-parser": "^2.0.0",
    "async-sema": "^3.1.0",
    "github-slugger": "^1.3.0",
    "globby": "^11.0.3",
    "katex": "^0.12.0",
    "next": "^11.0.1",
    "prismjs": "^1.23.0",
    "react": "17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "17.0.1",
    "sitemap": "^6.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.7",
    "@types/katex": "^0.11.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hiro-mackay/froom.co.jp.git"
  }
}
